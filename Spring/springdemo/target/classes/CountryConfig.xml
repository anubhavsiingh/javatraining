<?xml version="1.0" encoding="UTF-8"?>

<beans
    xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:p="http://www.springframework.org/schema/p" 
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd"
        xmlns:context="http://www.springframework.org/schema/context"
        >
<context:annotation-config></context:annotation-config>


<bean id="c1" class="com.coforge.training.springdemo.model.Capital">

<!-- This Invokes setter method- setName() of Developer class -->
<property name="name" value="New Delhi"></property>
</bean>

<bean id="myCountry" class="com.coforge.training.springdemo.model.Country">

<!-- This Invokes setter method- setName() of Developer class -->
<property name="name" value="India"></property>
</bean>

</beans>